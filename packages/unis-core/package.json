{
  "name": "@unis/unis",
  "version": "1.1.11",
  "description": "Unis is a simpler and easier to use front-end framework than React",
  "main": "build/unis.js",
  "module": "build/unis.mjs",
  "types": "index.d.ts",
  "typings": "index.d.ts",
  "exports": {
    ".": {
      "require": "./build/unis.js",
      "import": "./build/unis.mjs"
    },
    "./jsx-runtime": "./jsx-runtime.js"
  },
  "scripts": {
    "build": "rimraf build && rimraf dist && rollup --config && pnpm dts",
    "build:dev": "cross-env NODE_ENV=development pnpm build",
    "test": "vitest run --coverage",
    "test:watch": "vitest -w",
    "dts": "tsc -p tsconfig.build.json && api-extractor run --local --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anuoua/unis.git"
  },
  "keywords": [
    "frontend",
    "web",
    "framwork"
  ],
  "files": [
    "build",
    "jsx-runtime.d.ts",
    "jsx-runtime.js",
    "index.d.ts",
    "types"
  ],
  "author": "anuoua",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anuoua/unis/issues"
  },
  "homepage": "https://github.com/anuoua/unis#readme",
  "dependencies": {
    "@types/prop-types": "^15.7.4",
    "@types/scheduler": "^0.16.2",
    "csstype": "^3.0.10"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.18.19",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/jsdom": "^16.2.14",
    "c8": "^7.11.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.13.13",
    "jsdom": "^19.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.3",
    "rollup-plugin-esbuild": "^4.6.0",
    "rollup-plugin-reassign": "^1.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "vite": "^2.9.11",
    "vitest": "^0.21.0"
  }
}
