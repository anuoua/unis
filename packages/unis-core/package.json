{
  "name": "@unis/core",
  "version": "1.2.0-alpha.0",
  "description": "Unis is a simpler and easier to use front-end framework than React",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "typings": "index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./jsx-runtime": "./jsx-runtime.js",
    "./jsx-dev-runtime": "./jsx-dev-runtime.js"
  },
  "scripts": {
    "build": "rimraf build && rimraf dist && rollup --config && pnpm dts",
    "build:dev": "cross-env NODE_ENV=development pnpm build",
    "test": "vitest run --coverage",
    "test:watch": "vitest -w",
    "dts": "tsc -p tsconfig.build.json && api-extractor run --local --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anuoua/unis.git"
  },
  "keywords": [
    "frontend",
    "web",
    "framwork"
  ],
  "files": [
    "dist",
    "jsx-runtime.d.ts",
    "jsx-runtime.js",
    "jsx-dev-runtime.d.ts",
    "jsx-dev-runtime.js",
    "index.d.ts",
    "types"
  ],
  "author": "anuoua",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anuoua/unis/issues"
  },
  "homepage": "https://github.com/anuoua/unis#readme",
  "dependencies": {
    "@types/prop-types": "^15.7.5",
    "@types/scheduler": "^0.16.2",
    "csstype": "^3.1.1"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.34.4",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@swc/core": "^1.3.35",
    "@types/jsdom": "^21.1.0",
    "@vitest/coverage-c8": "^0.28.5",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.8",
    "jsdom": "^21.1.0",
    "rimraf": "^4.1.2",
    "rollup": "^3.17.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.2",
    "vitest": "^0.28.5"
  }
}
