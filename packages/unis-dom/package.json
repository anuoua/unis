{
  "name": "@unis/dom",
  "version": "1.2.0-alpha.0",
  "description": "Unis is a simpler and easier to use front-end framework than React",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "rimraf build && rimraf dist && rollup --config && pnpm dts",
    "build:dev": "cross-env NODE_ENV=development pnpm build",
    "test": "vitest run --coverage",
    "test:watch": "vitest -w",
    "dts": "tsc -p tsconfig.build.json && api-extractor run --local --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anuoua/unis.git"
  },
  "keywords": [
    "frontend",
    "web",
    "framwork"
  ],
  "files": [
    "dist"
  ],
  "author": "anuoua",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anuoua/unis/issues"
  },
  "homepage": "https://github.com/anuoua/unis#readme",
  "peerDependencies": {
    "@unis/core": "workspace:^"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.34.4",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@swc/core": "^1.3.35",
    "@types/jsdom": "^21.1.0",
    "@unis/core": "workspace:^",
    "@vitest/coverage-c8": "^0.28.5",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.8",
    "jsdom": "^21.1.0",
    "rimraf": "^4.1.2",
    "rollup": "^3.17.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-reassign": "^1.0.3",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.2",
    "vitest": "^0.29.8"
  }
}
